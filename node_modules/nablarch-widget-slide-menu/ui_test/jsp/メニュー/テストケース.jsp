<t:ui_test_template
  title="メニューのテスト"
  testcase="テストケース.js"
  testpage="テストページ.jsp"
  testdata="テストデータ.jsp">
  <jsp:attribute name="localCss">
    #defaultContext, #fromLeftContext {
      left:-320px;
    }
    #fromRightContext {
      right:-320px;
    }
    div.dummyMenu {
      z-index:200;
      position:fixed;
      top:30px;
      background-color:white;
    }
    .trigger {
      cursor:pointer;
    }
    div.hide {
      display:none;
    }
    div.nablarch_screen {
      display:block;
    }
    .touchdevice {
      border: double 4px #ff0000;
    }
  </jsp:attribute>
  
</t:ui_test_template>